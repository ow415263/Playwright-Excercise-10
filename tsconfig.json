{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
  "moduleResolution": "Node",
  "types": ["node", "@playwright/test"],
  "lib": ["ES2022", "DOM"],
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules", "dist", "output"]
}
